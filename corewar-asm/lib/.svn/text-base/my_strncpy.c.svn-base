char	*my_strncpy(char *dest, char *src, int n)
{
  int	i;

  i = 0;
  while (dest && src && src[i] && i < n)
    {
      dest[i] = src[i];
      i++;
    }
  if (dest)
    dest[i] = 0;
  return (dest);
}
